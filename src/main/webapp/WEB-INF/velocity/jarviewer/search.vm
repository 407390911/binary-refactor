<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>$!query in $!jarFile.OrigName</title>
    <meta name="author" content="Argan Wang">

    <!-- Le HTML5 shim, for IE6-8 support of HTML elements -->
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
    
    <!-- Le styles -->
    <link href="/static/css/bootstrap-1.4.0.css" rel="stylesheet">
    <!-- Le fav and touch icons -->
    <link rel="shortcut icon" href="images/favicon.ico">
    
  </head>
  <body>
    #include("/menu.vm")
        <div class="content">
        <div class="row">
        <div class="hero-unit">
            <h3>Class name match results</h3>
            <table>
            <tr>
              <td>序号</td><td>类名</td>
            </tr>
            #if($matches && $matches.size()>0)
            #foreach($f in $matches)
             <tr>
               <td>$velocityCount</td>
               <td><a href="view.htm?id=$!{id}&clz=$f">$f</a></td>
             </tr>
            #end
            #else
             <tr><td>No class mathes yet. </td></tr>
            #end
            </table>
        </div>
        </div>
        </div>
        #include("/footer.vm")
    </body>
</html>