<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>List of uploaded jars</title>
    <meta name="author" content="Argan Wang">

    <!-- Le HTML5 shim, for IE6-8 support of HTML elements -->
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

    <!-- Le styles -->
    <link href="/static/css/bootstrap-1.4.0.css" rel="stylesheet">
    <style type="text/css">
      body {
        padding-top: 60px;
      }
    </style>
    <!-- Le fav and touch icons -->
    <link rel="shortcut icon" href="images/favicon.ico">
  </head>
  <body>
    #include("/menu.vm")
	    <div class="content">
        <!-- Main hero unit for a primary marketing message or call to action -->
        <div class="hero-unit">
          <h3>Uploaded files</h3>
        <table>
        <tr>
          <td>id</td><td>type</td><td>name</td>
        </tr>
        #if($list && $list.size()>0)
        #foreach($f in $list)
		 <tr><td>$velocityCount</td><td>$f.type</td><td><a href="/jarviewer/view.htm?id=$f.id">$f.obj.origName</a></td></tr>
		#end
		#else
		 <tr><td>No files yet. <a href="/jarviewer/form.htm">Upload</a></td></tr>
		#end
		</table>
        </div>
        #include("/footer.vm")
    </body>
</html>